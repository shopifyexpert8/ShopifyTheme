*ul{
    padding: 0;
}
img {
    width: 100%;
}
ul li{
    list-style-type: none;
}
ul>li a {
    text-decoration: none;
}
    
.product-card {
    box-sizing: border-box;
    float: left;
    min-height: 1em;
    vertical-align: top;
    width: 25%;
  }

  .visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
    white-space: nowrap;
  }
  ul {
      display: block;
      list-style-type: none;
  }
  .custom-title h1 {
      text-align:center;
  }